{
  "_version": "1.59.0",
  "sap.app": {
    "id": "com.epic.shell",
    "type": "application",
    "title": "Epic Shell Host",
    "description": "Container for Integration Cards"
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "com.epic.shell.Shell",
      "type": "XML",
      "async": true,
      "id": "shellView"
    },
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.integration": {}
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "com.epic.cards.stalkerURL.view",
        "controlId": "stalkerCard",
        "controlAggregation": "items",
        "async": true
      },
      "routes": [
        {
          "name": "home",
          "pattern": "",
          "target": "platform"
        },
        {
          "name": "mirror",
          "pattern": "mirror/{id}",
          "target": "mirror"
        }
      ],
      "targets": {
        "platform": {
          "viewName": "Platform",
          "clearControlAggregation": true
        },
        "mirror": {
          "viewName": "MirrorWorld",
          "clearControlAggregation": true
        }
      }
    }
  }
}